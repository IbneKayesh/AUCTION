<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>@ViewBag.Title</title>

    <!-- Favicons -->
    <link href="../Dashio/img/favicon.png" rel="icon">
    <link href="../Dashio/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Bootstrap core CSS -->
    <link href="../Dashio/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!--external css-->
    <link href="../Dashio/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />

    <!--date time-->
    <link rel="stylesheet" type="text/css" href="../Dashio/lib/bootstrap-datepicker/css/datepicker.css" />
    <link rel="stylesheet" type="text/css" href="../Dashio/lib/bootstrap-datetimepicker/css/datetimepicker.css" />

    <!--select 2-->
    <link href="../Dashio/lib/select2/css/select2.min.css" rel="stylesheet" />
    <!--swal-->
    <link href="../Dashio/lib/swal/css/sweetalert2.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="../Dashio/css/style.css" rel="stylesheet">
    <link href="../Dashio/css/style-responsive.css" rel="stylesheet">

    <!--data tables-->
    <link rel="stylesheet" href="../Dashio/lib/advanced-datatable/css/DT_bootstrap.css" />

    <!--kayesh css-->
    <link href="../Dashio/css/customcss.css" rel="stylesheet" />

</head>

<body>
    <section id="container">
        <!-- **********************************************************************************************************************************************************
                    TOP BAR CONTENT & NOTIFICATIONS
        *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a href="@Url.Action("Index", "Home")" class="logo"><b>A<span>uction</span></b></a>
            <!--logo end-->
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="@Url.Action("Logout","Home")"><i class="fa fa-lock"></i> Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!-- **********************************************************************************************************************************************************
            MAIN SIDEBAR MENU
            *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">
                    <p class="centered"><a href="#"><img src="../Dashio/img/ui-sam.jpg" class="img-circle" width="80"></a></p>
                    <h5 class="centered">
                        @if (Session["UserId"] == null)
                    { <strong>Not Define</strong> }
                    else
                    { <strong>@Session["UserId"]</strong>}
                </h5>
                <li class="mt">
                    <a href="@Url.Action("Index","Home")">
                        <i class="fa fa-dashboard"></i>
                        <span>Home Page</span>
                    </a>
                </li>
                <li class="sub-menu">
                    <a href="#">
                        <i class="fa fa-desktop"></i>
                        <span>Default</span>
                    </a>
                    <ul class="sub">
                        <li>@Html.ActionLink("Change Password", "ChangePassword", "Home")</li>
                    </ul>
                </li>
                @if (Session["MenuList"] != null)
                {
                    var MenuMaster = (List<auction.Models.MNUP_MNUC>)Session["MenuList"];
                    var groupbyMenu = MenuMaster.GroupBy(x => x.MNUP_NAME).ToList();
                    foreach (var MenuList in groupbyMenu)
                    {
                        <li class="sub-menu">
                            <a href="#">
                                <i class="@MenuList.FirstOrDefault().MNUP_ICON"></i>
                                <span>@MenuList.Key</span>
                            </a>
                            <ul class="sub">
                                @foreach (var SubMenuList in MenuList)
                                {
                                    <li><a href="@Url.Action(SubMenuList.MNUC_ACTN,SubMenuList.MNUC_CONT)"><i class="@SubMenuList.MNUC_ICON"></i>@SubMenuList.MNUC_NAME</a></li>
                                }
                            </ul>
                        </li>
                    }
                }
            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    <!-- **********************************************************************************************************************************************************
        MAIN CONTENT
        *********************************************************************************************************************************************************** -->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i> @ViewBag.Title</h3>
            <div class="row mt">
                <div class="col-lg-12">
                    @RenderBody()
                </div>
            </div>
        </section>
        <!-- /wrapper -->
    </section>
    <!-- /MAIN CONTENT -->
    <!--main content end-->
    <!--footer start-->
    <footer class="site-footer">
        <div class="text-center">
            <p>
                &copy; Copyrights <strong>PRG MIS</strong>. All Rights Reserved
            </p>
            <div class="credits">
                PRG MIS
            </div>
            <a href="blank.html#" class="go-top">
                <i class="fa fa-angle-up"></i>
            </a>
        </div>
    </footer>
    <!--footer end-->
</section>
<!-- js placed at the end of the document so the pages load faster -->
@*<script src="../Dashio/lib/jquery/jquery.min.js"></script>*@
<!--data table js-->
@*<script type="text/javascript" language="javascript" src="../Dashio/lib/advanced-datatable/js/jquery.js"></script>*@
<script src="../Dashio/lib/jquery/jquery-3.5.1.min.js"></script>

<script src="../Dashio/lib/bootstrap/js/bootstrap.min.js"></script>
<script src="../Dashio/lib/jquery-ui-1.9.2.custom.min.js"></script>
<script src="../Dashio/lib/jquery.ui.touch-punch.min.js"></script>
<script class="include" type="text/javascript" src="../Dashio/lib/jquery.dcjqaccordion.2.7.js"></script>
<script src="../Dashio/lib/jquery.scrollTo.min.js"></script>
<script src="../Dashio/lib/jquery.nicescroll.js" type="text/javascript"></script>

<!--data table js-->
<script type="text/javascript" language="javascript" src="../Dashio/lib/advanced-datatable/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="../Dashio/lib/advanced-datatable/js/DT_bootstrap.js"></script>

<!--select 2-->
<script src="../Dashio/lib/select2/js/select2.min.js"></script>
<!--swal-->
<script src="../Dashio/lib/swal/js/sweetalert2.min.js"></script>
<!--Orverlay-->
<script src="../Dashio/lib/overlay/loadingoverlay.min.js"></script>

<!--date time-->
<script type="text/javascript" src="~/Dashio/lib/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
<script type="text/javascript" src="~/Dashio/lib/bootstrap-daterangepicker/date.js"></script>
<script type="text/javascript" src="~/Dashio/lib/bootstrap-daterangepicker/moment.min.js"></script>
<script type="text/javascript" src="~/Dashio/lib/bootstrap-timepicker/js/bootstrap-timepicker.js"></script>
<script src="~/Dashio/lib/advanced-form-components.js"></script>


<!--common script for all pages-->
<script src="../Dashio/lib/common-scripts.js"></script>
<!--script for this page-->
<script src="~/Scripts/Plugin/loadingoverlay.min.js"></script>
@RenderSection("scripts", required: false)

<script>
    //Sweet Alert
    @Html.Raw(TempData["msg"])

</script>
</body>

</html>
